    "rxjs": "5.4.3",
    "typescript": "2.4.2",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.3.1",
    "@angular/compiler-cli": "4.0.0",
    "@types/jasmine": "2.5.54",
