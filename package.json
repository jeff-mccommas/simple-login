{
  "name": "simplified-login",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build:client": "ng build",
    "build:server": "tsc -p ./tsconfig.server.json",
    "test:server": "mocha 'test/**/*.test.ts' --compilers js:ts-node/register,tsx:ts-node/register",
    "build": "npm run build:client && npm run build:server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "node dist/server/app.js",
    "serve:pm2": "pm2 start dist/server/app.js",
    "build:client:watch": "ng build --watch",
    "serve:watch": "nodemon dist/server/app.js",
    "start": "npm run build && npm run serve",
    "start:pm2": "npm run build && npm run serve:pm2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.5",
